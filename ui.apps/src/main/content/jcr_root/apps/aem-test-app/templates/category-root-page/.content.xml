<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:description="Template for initial or main page of the site."
          jcr:primaryType="cq:Template"
          jcr:title="bachelorproef-van-mike Home Page"
          allowedPaths="[/content/aem-test-content]"
          ranking="{Long}100">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            sling:resourceType="aem-test-app/components/structure/page">
        <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aem-test-app/components/structure/logo"/>
        <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aem-test-app/components/content/title"/>
        <contentnav
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aem-test-app/components/structure/contentnav"/>
        <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys">
            <categoryList
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aem-test-app/components/content/categoryList"/>
        </par>
    </jcr:content>
</jcr:root>
